{
  "name": "sql-completion",
  "version": "1.0.0",
  "description": "SQL completion service with WebSocket support",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsx src/server.ts",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/server.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexKaiqi/sql-completion.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexKaiqi/sql-completion/issues"
  },
  "homepage": "https://github.com/AlexKaiqi/sql-completion#readme",
  "dependencies": {
    "@types/node": "^20.11.24",
    "@types/uuid": "^10.0.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dt-sql-parser": "^4.1.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.13",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.60",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.1.0",
    "tsx": "^4.19.3",
    "vitest": "^3.1.1"
  }
}
